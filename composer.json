{
  "name": "markette/gopay-api",
  "description": "Public GopayAPI SDK for PHP.",
  "keywords": [
    "eshop",
    "payment",
    "gopay",
    "api",
    "sdk"
  ],
  "homepage": "https://github.com/contributte/gopay-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Vojtěch Dobeš",
      "email": "me@vojtechdobes.com"
    },
    {
      "name": "Jan Škrášek",
      "email": "hrach.cz@gmail.com"
    },
    {
      "name": "Milan Felix Šulc",
      "email": "sulcmil@gmail.com"
    },
    {
      "name": "Jaroslav Hranička",
      "email": "hranicka@outlook.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-soap": "*"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "nette/tester": "^2.3.2",
    "ninjify/nunjuck": "^0.3",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Markette\\Gopay\\Api\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.1.x-dev"
    }
  }
}
